# Configurações do Pipeline
ENVIRONMENT=development

# URLs das APIs de Dados Abertos de BH
BH_OPEN_DATA_BASE_URL=https://dados.pbh.gov.br
BH_ONIBUS_TEMPO_REAL_URL=https://temporeal.pbh.gov.br/v1/posicoes
BH_MCO_URL=https://dados.pbh.gov.br/dataset/mapa-de-controle-operacional-mco

# Diretórios de Dados
DATA_BRONZE_PATH=./data/bronze
DATA_SILVER_PATH=./data/silver
DATA_GOLD_PATH=./data/gold

# Configurações de Logs
LOG_LEVEL=INFO
LOG_PATH=./logs

# Configurações de Qualidade de Dados
ENABLE_DATA_QUALITY_CHECKS=true
ENABLE_DATA_LINEAGE=true

# Configurações de Agendamento
SCHEDULE_ENABLED=false
SCHEDULE_INTERVAL_MINUTES=15
